import java.util.*;

class Main {
    public static void main(String[] args) {
        System.out.println("Enter Count Number :");
        int count;
        Scanner sc = new Scanner(System.in);
        count=sc.nextInt();
        int num,avg=0,sum=0;
        for(int a=0;a<count;a++){
          num=sc.nextInt();
          sum = sum+num;
          avg= sum/count;
        }
        System.out.println("Average Number :"+avg);
    }
}
